<form action="/commander" method="post" enctype="multipart/form-data">
    <div class="form-horizontal">

        <div class="form-group row">
            <label class="col-form-label col-sm-2" for="Nom">Nom</label>
            <div class="col-sm-8">
                <input autofocus class="form-control" name="nom" value="<%= nom %>"/>
            </div>
        </div>
        
        <div class="form-group row">
            <label class="col-form-label col-sm-2" for="Prénom">Prénom</label>
            <div class="col-sm-7">
                <input class="form-control" name="prenom" value="<%= prenom %>" />
            </div>
        </div>

        <div class="form-group row">
            <label class="col-form-label col-sm-2" for="Adresse">Adresse</label>
            <div class="col-sm-8">
                <input autofocus class="form-control" name="adresse" value="<%= adresse %>" />
            </div>
        </div>
        
        <div class="form-group row">
            <label class="col-form-label col-sm-2" for="Télephone">Téléphone</label>
            <div class="col-sm-7">
                <input class="form-control" name="tel" value="<%= tel %>" />
            </div>
        </div>
        
        <div class="form-group row">
            <label class="col-form-label col-sm-2" for="email">Email</label>
            <div class="col-sm-7">
                <input class="form-control" name="email" value="<%= email %>" />
            </div>
        </div>

        <div class="form-group row">
            <label class="col-form-label col-sm-2"></label>
            <div class="col-sm-10">
                <input type="submit" value="Commander" class="btn btn-default btn-warning" />
                <a class="btn btn-outline-dark cancel" href="/">Annuler</a>
            </div>
        </div>
    </div>
  </form>